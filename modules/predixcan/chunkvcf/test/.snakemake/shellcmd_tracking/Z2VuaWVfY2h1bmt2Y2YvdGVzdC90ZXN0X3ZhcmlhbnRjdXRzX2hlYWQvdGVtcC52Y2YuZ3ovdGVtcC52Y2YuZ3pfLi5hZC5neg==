cat genie_chunkvcf/test/test_variantcuts/temp.vcf.gz/header genie_chunkvcf/test/test_variantcuts_nohead/temp.vcf.gz/temp.vcf.gz_..ad > genie_chunkvcf/test/test_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..ad 
bgzip genie_chunkvcf/test/test_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..ad && tabix -p vcf genie_chunkvcf/test/test_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..ad.gz