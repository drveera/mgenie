cat genie_chunkvcf/test2/test2_variantcuts/temp.vcf.gz/header genie_chunkvcf/test2/test2_variantcuts_nohead/temp.vcf.gz/temp.vcf.gz_..aa > genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..aa 
bgzip genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..aa && tabix -p vcf genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..aa.gz