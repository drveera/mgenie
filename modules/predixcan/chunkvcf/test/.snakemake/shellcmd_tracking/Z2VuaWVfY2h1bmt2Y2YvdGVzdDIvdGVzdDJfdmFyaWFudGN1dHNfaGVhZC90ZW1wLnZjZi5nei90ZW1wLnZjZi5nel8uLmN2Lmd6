cat genie_chunkvcf/test2/test2_variantcuts/temp.vcf.gz/header genie_chunkvcf/test2/test2_variantcuts_nohead/temp.vcf.gz/temp.vcf.gz_..cv > genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..cv 
bgzip genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..cv && tabix -p vcf genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..cv.gz