bgzip genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..cp && tabix -p vcf genie_chunkvcf/test2/test2_variantcuts_head/temp.vcf.gz/temp.vcf.gz_..cp.gz